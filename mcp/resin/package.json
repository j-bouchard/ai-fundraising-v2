{
  "name": "resin",
  "version": "0.0.1",
  "description": "Resin (built with ModelFetch)",
  "private": true,
  "type": "module",
  "scripts": {
    "mcp-server": "modelfetch dev",
    "stdio": "node --experimental-specifier-resolution=node --loader ts-node/esm src/stdio.ts",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "delete": "wrangler delete",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:unit": "vitest run tests/unit",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.17.4",
    "@modelfetch/cloudflare": "1.0.13",
    "tslib": "2.8.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@vitest/ui": "^3.2.4",
    "modelfetch": "1.0.13",
    "typescript": "5.9.2",
    "vitest": "^3.2.4",
    "wrangler": "4.33.1"
  }
}
